{
  "name": "doctorenv-cli",
  "version": "0.0.7",
  "description": "Helps you to make easy to onboard new developers to your project.",
  "main": "src/index.js",
  "bin": {
    "doctorenv": "./bin"
  },
  "scripts": {
    "start": "tsx ./src/index.js",
    "dev": "tsx --watch ./src/index.js",
    "dev:example": "npm run dev check example.config.js",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "build": "swc ./src -d ./bin",
    "publish": "npm run build && np"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "keywords": [
    "cli",
    "tool",
    "checker",
    "doctor"
  ],
  "author": "raphael kieling <https://github.com/raphaelkieling>",
  "license": "MIT",
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.93",
    "@vitest/coverage-v8": "^0.34.6",
    "chalk": "^5.3.0",
    "esbuild": "^0.19.4",
    "np": "^8.0.4",
    "tsx": "^3.13.0",
    "vitest": "^0.34.6",
    "yargs": "^17.7.2"
  }
}
